{
  "compilerOptions": {
    "outDir": "./dist",
    "rootDir": "src",
    "target": "ESNext",
    "lib": [
      "ESNext"
    ],
    "module": "commonjs",
    "esModuleInterop": true,
    "strict": true,
    "skipLibCheck": true,
    "declaration": true,
    "declarationMap": false,
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true,
    "noImplicitAny": false,
    "suppressImplicitAnyIndexErrors": false,
    "listFiles": false,
    "noImplicitUseStrict": false,
    "types": [
      "@cloudflare/workers-types"
    ]
  }
}