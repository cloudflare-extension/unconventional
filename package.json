{
  "name": "unconventional",
  "version": "0.0.13",
  "description": "A complete server built on Cloudflare Pages Functions",
  "keywords": [
    "cloudflare",
    "pages",
    "functions",
    "workers",
    "edge",
    "server",
    "express",
    "crud",
    "rest",
    "api"
  ],
  "author": "Dean Mauro",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cloudflare-extension/unconventional.git"
  },
  "bugs": {
    "url": "https://github.com/cloudflare-extension/unconventional/issues"
  },
  "homepage": "https://github.com/cloudflare-extension/unconventional#readme",
  "files": [
    "dist"
  ],
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "clean": "tsc --build --clean",
    "build": "rm -rf dist && tsc --build",
    "watch": "tsc --watch",
